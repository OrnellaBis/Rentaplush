<h1>BOOKING</h1>

<ul>
    <% @bookings.each do |booking| %>
        <li>Book commence <%= booking.start_date %> et fini <%= booking.end_date %></li>
        <%= link_to booking_path(booking), method: :delete do %>
            <i class="fas fa-trash-alt"></i>
        <% end %>
        
    <% end %>
</ul>